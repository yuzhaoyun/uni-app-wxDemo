<view class="main"><my-title vue-id="1" bind:__l="__l"></my-title><view class="nav-wrap"><scroll-view class="nav" scroll-x><block wx:for="{{navList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['activeNav',[index]]]]]}}" class="{{['nav-item',index===currentIndexNav?'active':'']}}" bindtap="__e">{{''+item.text+''}}</view></block></scroll-view></view><view class="slides"><swiper indicator-dots="{{true}}" autoplay="{{true}}" interval="{{2500}}" duration="{{500}}" circular="true"><block wx:for="{{swiperList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><swiper-item><image src="{{item.imgSrc}}" mode="widthFix"></image></swiper-item></block></swiper></view><view class="video-wrap"><block wx:for="{{videosList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><navigator class="video-item" url="../detail/detail" data-event-opts="{{[['tap',[['toDetail',[index]]]]]}}" bindtap="__e"><view class="video-img"><image class="video-img-image" src="{{item.imgSrc}}" mode="widthFix"></image><view class="video-info"><view class="play-count"><image class="count-image" src="data:image/png;base64," mode="widthFix"></image><text class="play-count">{{item.playCount}}</text></view><view class="comment-count"><image class="count-image" src="data:image/png;base64," mode="widthFix"></image><text class="comment-count">{{item.commentCount}}</text></view></view></view><view class="video-title">{{item.desc}}</view></navigator></block></view></view>